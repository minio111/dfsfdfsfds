{
  "manifest_version": 2,
  "name": "moneyy",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": ["background.js"]

  },
  "browser_action": {
    "default_icon": "coin.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "web_accessible_resources": [
    "calculations.js"
  ]
}
